<body>

    <!-- Modal -->
    <div class="modal fade" id="createEventModal" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Create Event</h4>
                </div>
                <div class="modal-body">
                    <div id="eventForm" style="margin: 2em;">

                        <form>

                            <div style="width:450px; margin-top: 2.5em;">
                                <div class="input-group" style="margin: 1.5em;">
                                    <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                                    <input class="form-control" id="event_name" type="text" name="name" value="" placeholder="Event Name" data-toggle="tooltip" data-placement="right" />
                                </div>
                                
                                   <div class="input-group" style="margin: 1.5em;">
                                    <textarea class="form-control" id="event_desc" type="text" placeholder="Event Description" data-content="" data-toggle="tooltip" data-placement="right" rows="10" />
                                </div>
                                
                                <div id="event_start" class="input-group" style="margin: 1.5em;">
                                    <div>
                                        <input type="number" id="start_date" min="1" max="31">
                                    </div>
                                    <div>
                                        <input type="month" id="start_month">
                                    </div>
                                </div>

                                <div id="event_end" class="input-group" style="margin: 1.5em;">
                                    <div>
                                        <input type="number" id="end_date" min="1" max="31">
                                    </div>
                                    <div>
                                        <input type="month" id="end_month">
                                    </div>
                                </div>

                                <div style="margin: 1.5em;">
                                    <p class="text-center">
                                        <!--button click connect to db-->
                                        <button class="btn btn-success" id="login-btn" name="commit" value="request" data-analytics="SignupPassword" style="background-color: darkseagreen;"><font size="3" color="white">Log In</font></button>
                                    </p>
                                    <hr>
                                </div>


                            </div>

                        </form>

                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" id="signupModal_btn" class="btn btn-default" data-dismiss="modal">Proceed to Login</button>
                </div>
            </div>

        </div>
    </div>

<h1>here we are</h1>

